{
    "workbench.colorTheme": "Monokai",
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": "{{put-password-here}}"
        }
    ],
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "editor.autoIndent": "none",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.links": false,
    "editor.formatOnSave": true,
    "editor.fontSize": 12,
    "terminal.integrated.shell.osx": "bash",
    "explorer.openEditors.visible": 0,
    "breadcrumbs.enabled": false,
    "terminal.integrated.inheritEnv": true,
    "workbench.preferredDarkColorTheme": "Monokai",
    "workbench.preferredLightColorTheme": "Default Dark+",
    "workbench.editor.closeEmptyGroups": false,
    "window.autoDetectColorScheme": true,
    "workbench.colorCustomizations": {
        "terminal.background": "#000000",
        "editor.background": "#000000",
        "terminal.foreground": "#C0C0C0",
        "terminalCursor.background": "#C0C0C0",
        "terminalCursor.foreground": "#C0C0C0",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#0303f1",
        "terminal.ansiBrightBlack": "#808080",
        "terminal.ansiBrightBlue": "#0000D3",
        "terminal.ansiBrightCyan": "#02ABEA",
        "terminal.ansiBrightGreen": "#1FB714",
        "terminal.ansiBrightMagenta": "#4700A5",
        "terminal.ansiBrightRed": "#c11a18",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "terminal.ansiBrightYellow": "#FBF305",
        "terminal.ansiCyan": "#02ABEA",
        "terminal.ansiGreen": "#1FB714",
        "terminal.ansiMagenta": "#4700A5",
        "terminal.ansiRed": "#03b129",
        "terminal.ansiWhite": "#C0C0C0",
        "terminal.ansiYellow": "#FBF305"
    },
    "workbench.editor.centeredLayoutAutoResize": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.copyOnSelection": true,
    "copyOnSelect.trimStart": false,
    // Should trim the end of the copied text?
    "copyOnSelect.trimEnd": false,
    "workbench.editor.openPositioning": "left",
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.panel.defaultLocation": "bottom",
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100,
    "terminal.integrated.experimentalLinkProvider": false,
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": 1,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "window.openWithoutArgumentsInNewWindow": "on",
    "editor.renderWhitespace": "none",
    "workbench.activityBar.visible": false,
    "editor.minimap.enabled": false,
    "remote.SSH.enableAzureIntegration": true,
    "remote.SSH.remoteServerListenOnSocket": true,
    "remote.SSH.enableAgentForwarding": false,
    "terminal.integrated.automationShell.linux": "",
    "terminal.integrated.automationShell.windows": "",
    "launch": {
        "configurations": [],
        "compounds": []
    },
    "terminal.integrated.automationShell.osx": "",
    "terminal.integrated.env.osx": {},
    "editor.codeLens": false,
    "outline.showStructs": false,
    "editor.formatOnPaste": true,
    "workbench.settings.openDefaultKeybindings": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "settingsSync.ignoredExtensions": [],
    "workbench.settings.openDefaultSettings": true,
    "debug.javascript.debugByLinkOptions": null,
    "terminal.integrated.macOptionClickForcesSelection": true,
    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash"
        },
        "zsh": {
            "path": "zsh"
        },
        "fish": {
            "path": "fish"
        },
        "tmux": {
            "path": "tmux"
        },
        "pwsh": {
            "path": "pwsh"
        }
    },
    "terminal.integrated.sendKeybindingsToShell": true,
    "go.toolsManagement.autoUpdate": true,
    "copyOnSelect.copyOnMouseSelection": false,
}
