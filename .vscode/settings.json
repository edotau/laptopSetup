{
    "workbench.colorTheme": "Monokai",
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": "{{put-password-here}}"
        }
    ],
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "editor.autoIndent": "none",
    "editor.formatOnPaste": true,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.links": false,
    "editor.formatOnSave": true,
    "editor.fontSize": 12,
    "terminal.integrated.shell.osx": "/usr/local/bin/bash",
    "settingsSync.ignoredSettings": [
        "terminal.external.osxExec",
    ],
    "terminal.integrated.inheritEnv": false,
    "explorer.openEditors.visible": 0,
    "breadcrumbs.enabled": false,
    "workbench.preferredDarkColorTheme": "Monokai",
    "workbench.preferredLightColorTheme": "Default Dark+",
    "workbench.editor.closeEmptyGroups": false,
    "window.autoDetectColorScheme": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "workbench.colorCustomizations": {
        "terminal.background": "#000000",
        "editor.background": "#000000",
        "terminal.foreground": "#C0C0C0",
        "terminalCursor.background": "#C0C0C0",
        "terminalCursor.foreground": "#C0C0C0",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#0303f1",
        "terminal.ansiBrightBlack": "#808080",
        "terminal.ansiBrightBlue": "#0000D3",
        "terminal.ansiBrightCyan": "#02ABEA",
        "terminal.ansiBrightGreen": "#1FB714",
        "terminal.ansiBrightMagenta": "#4700A5",
        "terminal.ansiBrightRed": "#c11a18",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "terminal.ansiBrightYellow": "#FBF305",
        "terminal.ansiCyan": "#02ABEA",
        "terminal.ansiGreen": "#1FB714",
        "terminal.ansiMagenta": "#4700A5",
        "terminal.ansiRed": "#03b129",
        "terminal.ansiWhite": "#C0C0C0",
        "terminal.ansiYellow": "#FBF305"
    },
    "workbench.editor.centeredLayoutAutoResize": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.copyOnSelection": true,
    // Should copy the selected text when it was selected using the mouse?
    "copyOnSelect.copyOnMouseSelection": true,
    // Should trim the start of the copied text?
    "copyOnSelect.trimStart": false,
    // Should trim the end of the copied text?
    "copyOnSelect.trimEnd": true,
    "terminal.integrated.allowChords": false,
    "workbench.editor.openPositioning": "left",
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.panel.defaultLocation": "right",
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 100,
    "terminal.integrated.experimentalLinkProvider": false,
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": 1,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "window.openWithoutArgumentsInNewWindow": "on",
    "editor.renderWhitespace": "none",
}